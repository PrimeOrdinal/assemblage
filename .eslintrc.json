{
  "env": {
    "es6": true
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    },
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "plugins": [
    "prettier",
    "typescript"
  ]
}
